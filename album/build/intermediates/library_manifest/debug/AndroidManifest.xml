<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zj.album"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="24"
        android:targetSdkVersion="29" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application>
        <activity
            android:name="com.zj.album.graphy.activity.PhotoGraphActivity"
            android:launchMode="standard"
            android:process=":cf_album"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.zj.album.graphy.activity.PhotoPreviewActivity"
            android:launchMode="standard"
            android:process=":cf_album"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.zj.album.graphy.activity.FolderActivity"
            android:process=":cf_album" />
        <activity
            android:name="com.zj.album.graphy.activity.VideoPreviewActivity"
            android:process=":cf_album" />
    </application>

</manifest>